{
  "name": "fast-base64",
  "description": "Fastest possible base64 encoding/decoding using WebAssembly",
  "version": "0.1.0",
  "author": "gregor <gregor.mitscha-baude@gmx.at>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mitchabaude/fast-base64"
  },
  "keywords": [
    "base64",
    "wasm",
    "fast"
  ],
  "type": "module",
  "main": "./dist/base64-wasm.js",
  "exports": {
    ".": {
      "import": "./dist/base64-wasm.js",
      "require": "./dist/base64-wasm.js"
    },
    "./small": {
      "import": "./base64-wasm-small.js",
      "require": "./base64-wasm-small.js"
    },
    "./js": {
      "import": "./base64-js.js",
      "require": "./base64-js.js"
    },
    "./util": {
      "import": "./util.js",
      "require": "./util.js"
    }
  },
  "files": [
    "dist",
    "base64-js.js",
    "base64-wasm-small.js",
    "util.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "binaryen": "^101.0.0",
    "chrode": "^0.2.3",
    "esbuild": "^0.12.15",
    "eslint": "^7.31.0",
    "wabt": "^1.0.23"
  }
}
